Private Sub cmdLanceDemo_Click()
    Set cCpt = MD_Demo.GetInstanceclsLabels()
    Dim bRep As Boolean

    If (Not Me.chkDebug) Then
        Me.txtLblOn = 0
        Me.txtLblOff = 0
        Me.txtDiff = 0
    End If

    With cCpt
        .xDebugMode = Me.chkDebug
        .xDebugActiveLabels = Me.chkActiveLabels

        bRep = .SetLabels(E_LblRang.Rang1, Me.lblU1, Me.lblD1, Me.lblC1)        '// Rang 1
        If (Not bRep) Then
            ResetCls
            If Me.chkDebug Then Stop        'TODO ERR msg Non assigné
            Exit Sub
        End If

        bRep = .SetLabels(E_LblRang.Rang2, Me.lblU2, Me.lblD2, Me.lblC2)        '// Rang 2
        If (Not bRep) Then
            If Me.chkDebug Then Stop        'TODO ERR msg Non assigné
            Exit Sub
        End If

        .SetLabelTexte Me.lblCompteInfo

    End With

    If (Me.chkDebug And (Me.chkActiveLabels = False)) Then Me.lblAttente.Visible = True
    DoEvents

    MD_Demo.LanceBoucle Me.txtBoucle

    If (cCpt.xDebugTimerEndLabelOn <> 0) Then Me.txtLblOn = cCpt.xDebugTimerEndLabelOn
    If (cCpt.xDebugTimerEndLabelOff <> 0) Then Me.txtLblOff = cCpt.xDebugTimerEndLabelOff
    Me.txtDiff = FormatNumber((CSng(Me.txtLblOn) - CSng(Me.txtLblOff)), 3, vbTrue, , vbUseDefault)

    Me.lblAttente.Visible = False
    cCpt.MasqueLabels Rang1
    cCpt.MasqueLabels Rang2
    Set cCpt = Nothing
    MD_Demo.ResetInstanceclsLabels
    Me.chkActiveLabels = True

    MsgBox "Démo terminée", vbInformation, "Demo"

End Sub

Private Sub ResetCls()
    Me.chkActiveLabels = True
    Me.lblAttente.Visible = False
    Set cCpt = Nothing
    MD_Demo.ResetInstanceclsLabels
End Sub
